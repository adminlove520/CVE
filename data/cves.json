          "type": "reference"
        }
      ],
      "affected": [
        {
          "vendor": "appsbd",
          "product": "Vitepos – Point of Sale (POS) for WooCommerce",
          "versions": [
            {
              "version": "*",
              "status": "affected",
              "lessThanOrEqual": "3.3.0",
              "versionType": "semver"
            }
          ],
          "defaultStatus": "unaffected"
        }
      ],
      "problemType": [
        "CWE-434 Unrestricted Upload of File with Dangerous Type"
      ],
