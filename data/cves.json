      "affected": [
        {
          "vendor": "Arksine",
          "product": "moonraker",
          "versions": [
            {
              "version": "< 0.10.0",
              "status": "affected"
            }
          ]
        }
      ],
      "problemType": [
        "CWE-90: Improper Neutralization of Special Elements used in an LDAP Query ('LDAP Injection')",
        "CWE-209: Generation of Error Message Containing Sensitive Information"
      ],
