        },
        {
          "url": "https://vulncheck.com/advisories/optilink-ont1gew-router-rce",
          "type": "reference"
        }
      ],
      "affected": [
        {
          "defaultStatus": "unaffected",
          "modules": [
            "Web Management Interface (formPing and formTracert diagnostic command handlers)",
            "target_addr (POST parameter in formTracert and formPing endpoints)"
